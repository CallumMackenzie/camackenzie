{
	"hosting": {
		"public": "build",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"headers": [
			{
				"source": "**/*.pdf",
				"headers": [
					{
						"key": "Content-Type",
						"value": "application/pdf"
					}
				]
			}
		],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	}
}